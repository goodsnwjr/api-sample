{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "user": {
    "dataSource": "mongoDs",
    "public": true,
    "options": {
      "emailVerificationRequired": false,
      "remoting": {
        "sharedMethods": {
          "*": false,
          "login": true,
          "logout": true,
          "sig": true,
          "create": true,
          "profile": true,
          "find": true,
          "count": true,
          "prototype.patchAttributes": true,
          "registBuyer": true,
          "updateBuyer": true,
          "findBuyerByCode": true
        }
      }
    }
  },
  "accessToken": {
    "dataSource": "mongoDs",
    "public": false,
    "relations": {
      "user": {
        "type": "belongsTo",
        "model": "user",
        "foreignKey": "userId"
      }
    }
  },
  "ACL": {
    "dataSource": "mongoDs",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongoDs",
    "public": false
  },
  "Role": {
    "dataSource": "mongoDs",
    "public": false
  },
  "board": {
    "dataSource": "mongoDs",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "create": true,
          "find": true,
          "prototype.patchAttributes": true,
          "deleteById": true
        }
      }
    }
  }
}
